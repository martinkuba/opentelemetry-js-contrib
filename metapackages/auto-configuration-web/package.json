{
  "name": "@opentelemetry/auto-configuration-web",
  "version": "0.1.0",
  "description": "Helper functions to simplify configuration of web instrumentation.",
  "author": "OpenTelemetry Authors",
  "homepage": "https://github.com/open-telemetry/opentelemetry-js-contrib/tree/main/metapackages/auto-configuration-web#readme",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "build/src/index.js",
  "module": "build/esm/index.js",
  "esnext": "build/esnext/index.js",
  "types": "build/src/index.d.ts",
  "repository": "open-telemetry/opentelemetry-js-contrib",
  "scripts": {
    "clean": "tsc --build --clean tsconfig.json tsconfig.esm.json tsconfig.esnext.json",
    "compile": "tsc --build tsconfig.json tsconfig.esm.json tsconfig.esnext.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "precompile": "tsc --version && lerna run version:update --scope @opentelemetry/auto-configuration-web --include-dependencies",
    "prewatch": "npm run precompile",
    "prepublishOnly": "npm run compile",
    "test:browser": "nyc karma start --single-run",
    "watch": "tsc --build --watch tsconfig.json tsconfig.esm.json tsconfig.esnext.json"
  },
  "bugs": {
    "url": "https://github.com/open-telemetry/opentelemetry-js-contrib/issues"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.6",
    "@babel/preset-env": "7.24.6",
    "@opentelemetry/api": "^1.3.0",
    "@types/mocha": "10.0.6",
    "@types/node": "18.6.5",
    "@types/sinon": "10.0.20",
    "@types/webpack-env": "1.16.3",
    "assert": "2.0.0",
    "babel-loader": "8.3.0",
    "babel-plugin-istanbul": "7.0.0",
    "cross-var": "1.1.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.2.1",
    "karma-mocha": "2.0.1",
    "karma-spec-reporter": "0.0.36",
    "karma-webpack": "5.0.1",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "rimraf": "5.0.10",
    "sinon": "15.1.2",
    "ts-loader": "9.5.1",
    "ts-mocha": "10.0.0",
    "typescript": "4.4.4",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-merge": "5.10.0"
  },
  "dependencies": {
    "@opentelemetry/api-events": "^0.53.0",
    "@opentelemetry/core": "^1.25.1",
    "@opentelemetry/instrumentation": "^0.53.0",
    "@opentelemetry/opentelemetry-browser-detector": "^0.53.0",
    "@opentelemetry/resources": "^1.26.0",
    "@opentelemetry/sdk-events": "^0.53.0",
    "@opentelemetry/sdk-logs": "^0.53.0",
    "@opentelemetry/sdk-trace-base": "^1.26.0",
    "@opentelemetry/sdk-trace-web": "^1.26.0",
    "@opentelemetry/semantic-conventions": "^1.27.0"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts",
    "build/esm/**/*.js",
    "build/esm/**/*.js.map",
    "build/esm/**/*.d.ts",
    "build/esnext/**/*.js",
    "build/esnext/**/*.js.map",
    "build/esnext/**/*.d.ts"
  ]
}
